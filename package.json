{
  "name": "multi-list-selection",
  "version": "0.1.1",
  "description": "Library for managing selections across multiple lists",
  "main": "dist/multi-list-selection.js",
  "scripts": {
    "test": "npm run lint && mocha --compilers js:babel-register test",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "lint": "eslint test src && echo 'linting passed'",
    "clean": "rm -rf dist",
    "build": "node_modules/.bin/babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kuychaco/multi-list-selection.git"
  },
  "keywords": [
    "multiple",
    "lists",
    "selection",
    "navigation"
  ],
  "author": "Katrina Uychaco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuychaco/multi-list-selection/issues"
  },
  "homepage": "https://github.com/kuychaco/multi-list-selection#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.5.3"
  }
}
